    if ($ip[0] == '[') {
        $ip = substr($ip, 1);
    } // ipv6 address
    if (!in_array($port, $portsInUse) && (!$bind || in_array($ip, $list))) {
        $portsInUse[] = $port;
    }
}
sort($portsInUse, SORT_NUMERIC);
